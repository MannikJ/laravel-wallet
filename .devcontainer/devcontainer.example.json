{
  "name": "Laradock",
  "dockerComposeFile": [
    "../../laradock/docker-compose.yml"
  ],
  "service": "workspace",
  "workspaceFolder": "/var/www/laravel-wallet",
  "extensions": [
    "artdiniz.quitcontrol-vscode",
